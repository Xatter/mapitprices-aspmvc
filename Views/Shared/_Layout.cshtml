<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/colors.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/layout.css")" rel="Stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" type="text/javascript""></script>
    <script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAAB02y8lF6cBk8AjbrJk0ISxQ6-9oJRf-Ib1VNSZsT4QL_Dhq6ohS1FpVo9YaXN7hSrvKEcbwGu8FVPQ"
        type="text/javascript"></script>
    <style type="text/css">
        .BingMap
        {
            height: 500px;
            border: solid 1px black;
        }
        .GoogleMap
        {
            height: 500px;
            border: solid 1px black;
        }
    </style>
</head>

<body>
    <div id="header" class="container_12">
        <div class="grid_12">
            <a href="/">
                <img src="../../Content/2011-06-17_Logo_TEMP.png" style="border: none" alt="Is this big enough? ... That's what she said" /></a>
        </div>
    </div>
    <div id="content" class="container_12">
        @RenderBody()
    </div>
    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-22338335-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

    </script>
</body>
</html>
